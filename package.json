{
  "name": "POServer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pm2": "pm2 start server/index.js --name \"pos\" --watch",
    "eco": "pm2 reload ecosystem.config.js --env=production",
    "server-dev": "cross-env NODE_ENV=test CLIENT_HOST=http://localhost:3000 nodemon server/index.js",
    "server-debug": "cross-env NODE_ENV=test CLIENT_HOST=http://localhost:3000 nodemon --inspect-brk server/index.js",
    "test": "cross-env NODE_ENV=test mocha --timeout 5000 __tests__/server/**/*.test.js",
    "report": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --timeout 5000 __tests__/server/**/*.test.js",
    "text-report": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha --timeout 5000 __tests__/server/**/*.test.js",
    "nyc": "cross-env NODE_ENV=test nyc mocha --timeout 5000 __tests__/server/**/*.test.js"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "diskspace": "^2.0.0",
    "dotenv": "^5.0.0",
    "express": "^4.15.3",
    "express-fileupload": "^0.4.0",
    "express-mailer": "^0.3.1",
    "express-promise-router": "^2.0.0",
    "i18next": "^10.5.0",
    "i18next-express-middleware": "^1.0.10",
    "image-downloader": "^3.4.1",
    "jade": "^1.11.0",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "module-alias": "^2.0.3",
    "mongodb-autoincrement": "^1.0.1",
    "mongoose": "^4.11.3",
    "morgan": "^1.8.2",
    "node-thumbnail": "^0.14.0",
    "nodemailer": "^4.4.2",
    "osmosis": "^1.1.7",
    "paginator": "^1.0.0",
    "passport": "^0.3.2",
    "passport-facebook-token": "^3.3.0",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "qs": "^6.5.2",
    "ramda": "^0.25.0",
    "sendpulse-api": "^1.0.3",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "faker": "^4.1.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "rewire": "^2.5.2",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.13.0"
  },
  "nyc": {
    "exclude": [
      "server/app.js",
      "server/configuration/index.js",
      "server/passport.js",
      "server/models/**/*.js",
      "server/helpers/**/*.js",
      "__tests__",
      "node_modules"
    ],
    "cache": false,
    "report-dir": "./__tests__/report"
  },
  "_moduleAliases": {
    "@root": ".",
    "@models": "server/models",
    "@controllers": "server/controllers",
    "@configuration": "server/configuration",
    "@mail": "server/mail"
  }
}
